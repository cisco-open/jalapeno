import (
  "log"
	"net/http"
)

type Server struct {
	url    string
	router *Router
}

func New(listen string) *Server {
	s := &Server{listen, newRouter()}
	s.router.InitRoutes(s)
	return s
}

func (s *Server) ListenAndServe() {
	err := http.ListenAndServe(s.url, s.router)
	if err != nil {
		log.Fatal("ListenAndServe: ", err)
	}
}


